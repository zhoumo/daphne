<?page contentType="text/html;charset=UTF-8"?>
<zk>
	<window title="用户管理" width="380px" height="184px" use="com.hebut.framework.ui.manage.member.UserWindow">
		<borderlayout>
			<center border="none">
				<grid>
					<columns>
						<column width="30%" align="center" />
						<column width="70%" />
					</columns>
					<rows>
						<row>
							<label value="用户登录号" />
							<textbox id="userLoginName" width="200px" constraint="no empty" />
						</row>
						<row>
							<label value="用户姓名" />
							<textbox id="userName" width="200px" constraint="no empty" />
						</row>
						<row>
							<label value="登录密码" />
							<textbox id="userPassword" width="200px" constraint="no empty" />
						</row>
					</rows>
				</grid>
			</center>
			<south>
				<hbox>
					<button id="submit" label="保存" />
					<button id="cancel" label="取消" />
				</hbox>
			</south>
		</borderlayout>
	</window>
</zk>